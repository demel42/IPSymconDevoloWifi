{
	"elements":
			[
				{ "type": "Label", "label": "name can be a hostname or ip-address" },
				{ "type": "ValidationTextBox", "name": "ap_name", "caption": "Name" },

				{ "type": "Label", "label": "must only be filled, if restricted access is configured" },
				{ "type": "ValidationTextBox", "name": "username", "caption": "Username" },
				{ "type": "ValidationTextBox", "name": "password", "caption": "Password" },

				{ "type": "Label", "label": "optional data of accesspoint" },
				{ "type": "CheckBox", "name": "with_dns", "caption": "resolve ip/hostname by DNS" },
				{ "type": "CheckBox", "name": "with_ap_detail", "caption": "Details" },

				{ "type": "Label", "label": "optional data of WLAN" },
				{ "type": "CheckBox", "name": "with_wlan_info", "caption": "Informations" },
				{ "type": "CheckBox", "name": "with_wlan_detail", "caption": "Details" },

				{ "type": "Label", "label": "optional data of Guest-WLAN" },
				{ "type": "CheckBox", "name": "with_guest_info", "caption": "Informations" },
				{ "type": "CheckBox", "name": "with_guest_detail", "caption": "Details" },

				{ "type": "Label", "label": "optional script to hide/show variables" },
				{ "type": "SelectScript", "name": "visibility_script", "caption": "visibility" },

				{ "type": "Label", "label": "Number of ethernet-port connected to LAN/WAN" },
				{ "type": "NumberSpinner", "name": "wan_port", "caption": "WAN-Port" },
				{ "type": "Label", "label": "MAC of the dLAN-Adapter next to ethernet" },
				{ "type": "ValidationTextBox", "name": "wan_bridge", "caption": "WAN-Bridge" },

				{ "type": "Label", "label": "query accesspoint every X minutes" },
				{ "type": "IntervalBox", "name": "UpdateDataInterval", "caption": "Minutes" }
			],
	"actions":
			[
				{ "type": "Label", "label": "one query makes take several seconds to complete" },
				{ "type": "Button", "label": "Perform query", "onClick": "DevoloAP_UpdateData($id);" }
			],		
	"status":
			[
				{ "code": 101, "icon": "inactive", "caption": "Instance getting created" },
				{ "code": 102, "icon": "active", "caption": "Instance is active" },
				{ "code": 104, "icon": "inactive", "caption": "Instance is inactive" },

				{ "code": 201, "icon": "error", "caption": "Instance is inactive (unauthorized)" },
				{ "code": 202, "icon": "error", "caption": "Instance is inactive (server error)" },
				{ "code": 203, "icon": "error", "caption": "Instance is inactive (http error)" },
				{ "code": 204, "icon": "error", "caption": "Instance is inactive (invalid data)" }
			]
}
